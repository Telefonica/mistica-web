{
    "name": "@telefonica/mistica",
    "version": "7.8.0",
    "license": "UNLICENSED",
    "main": "dist/index.js",
    "module": "dist-es/index.js",
    "files": [
        "dist/*",
        "dist-es/*",
        "css/*"
    ],
    "scripts": {
        "test": "jest --config jest.unit.config.js",
        "test-acceptance": "cross-env SCREENSHOT=1 HEADLESS=1 yarn test-acceptance-ui",
        "test-acceptance-ui": "jest --config jest.acceptance.config.js",
        "prettier-check": "prettier --check \"**/*.js\" \"**/*.tsx\" \"**/*.ts\" \"**/*.json\"",
        "lint": "eslint --report-unused-disable-directives .",
        "ts-check": "tsc --project tsconfig.json --noEmit",
        "prebuild": "yarn clean",
        "build": "node scripts/build.js",
        "postbuild": "yarn gen-ts-defs && yarn gen-flow-defs && node scripts/size-stats",
        "watch": "node scripts/watch.js",
        "clean": "rimraf dist && rimraf dist-es && rimraf dist-ts && rimraf node_modules/.cache",
        "ci": "yarn lint && yarn prettier-check && yarn build && yarn storybook-static && yarn test --ci && yarn test-acceptance --ci",
        "prepublishOnly": "node scripts/prepublish-only.js && yarn build",
        "storybook": "start-storybook -p 6006",
        "playroom": "playroom start",
        "gen-ts-defs": "rimraf dist-ts && tsc --project tsconfig.production.json --emitDeclarationOnly && cp -r dist-ts/src/* dist && rimraf dist-ts",
        "gen-flow-defs": "node scripts/dts-to-flow && yarn flow check",
        "now-build": "yarn storybook-static && ./node_modules/.bin/playroom build && cp img/favicon.ico public/",
        "storybook-static": "rimraf public && yarn build-storybook -o public --quiet",
        "up-chromium": "docker-compose up -d chromium",
        "down-chromium": "docker-compose stop chromium",
        "detect-circular-dependencies": "npx dpdm --circular src/index.tsx",
        "audit-accessibility": "node .github/actions/audit-accessibility/main.js"
    },
    "browserslist": [
        "chrome >= 36",
        "firefox >= 67",
        "safari >= 10.1",
        "edge >= 17",
        "ios_saf >= 10"
    ],
    "prettier": "@telefonica/prettier-config",
    "devDependencies": {
        "@actions/core": "^1.2.6",
        "@actions/github": "^4.0.0",
        "@axe-core/puppeteer": "^4.1.0",
        "@azure/storage-blob": "^12.2.1",
        "@babel/cli": "^7.12.1",
        "@babel/core": "^7.12.3",
        "@babel/parser": "^7.12.3",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
        "@babel/plugin-proposal-optional-chaining": "^7.12.1",
        "@babel/plugin-transform-modules-commonjs": "^7.12.1",
        "@babel/plugin-transform-property-literals": "^7.12.1",
        "@babel/preset-env": "^7.12.1",
        "@babel/preset-react": "^7.12.1",
        "@babel/preset-typescript": "^7.12.1",
        "@babel/traverse": "^7.12.1",
        "@semantic-release/changelog": "^5.0.1",
        "@semantic-release/git": "^9.0.0",
        "@storybook/addon-actions": "^6.0.27",
        "@storybook/addon-info": "^5.3.21",
        "@storybook/addon-links": "^6.0.27",
        "@storybook/addon-storysource": "^6.0.27",
        "@storybook/addon-viewport": "^6.0.27",
        "@storybook/addons": "^6.0.27",
        "@storybook/cli": "^6.1.10",
        "@storybook/components": "^6.0.27",
        "@storybook/react": "^6.0.28",
        "@storybook/theming": "^6.0.27",
        "@telefonica/eslint-plugin-async-await": "^1.0.0",
        "@telefonica/prettier-config": "^1.0.0",
        "@testing-library/jest-dom": "^5.11.5",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "@types/detect-port": "^1.3.0",
        "@types/jest-environment-puppeteer": "^4.4.0",
        "@types/jest-image-snapshot": "^4.1.3",
        "@types/mkdirp": "^1.0.1",
        "@types/puppeteer": "^5.4.0",
        "@types/react-autosuggest": "^10.0.1",
        "@types/react-router-dom": "^5.1.6",
        "@types/react-transition-group": "^4.4.0",
        "@types/resize-observer-browser": "^0.1.4",
        "@types/rimraf": "^3.0.0",
        "@types/semver-compare": "^1.0.1",
        "@typescript-eslint/eslint-plugin": "^4.11.0",
        "@typescript-eslint/parser": "^4.11.0",
        "axe-html-reporter": "^2.0.1",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-lodash": "^3.3.4",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-typescript-to-proptypes": "^1.4.1",
        "cpx": "^1.5.0",
        "cross-env": "^7.0.2",
        "detect-port": "^1.3.0",
        "eslint": "^7.16.0",
        "eslint-plugin-babel": "^5.3.1",
        "eslint-plugin-filenames": "^1.3.2",
        "eslint-plugin-flowtype": "^5.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.1.3",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-testing-library": "^3.10.1",
        "flow-bin": "^0.136.0",
        "flow-typed": "^3.2.1",
        "flowgen": "^1.12.0",
        "glob": "^7.1.6",
        "husky": ">=4.3.0",
        "is-ci": "^2.0.0",
        "jest": "^26.6.3",
        "jest-environment-puppeteer": "^4.4.0",
        "jest-image-snapshot": "^4.2.0",
        "jimp": "^0.16.1",
        "jscodeshift": "^0.11.0",
        "lint-staged": ">=10.5.0",
        "node-fetch": "^2.6.1",
        "playroom": "^0.22.3",
        "pptr-testing-library": "0.6.4",
        "prettier": "^2.1.2",
        "prop-types": "^15.7.2",
        "puppeteer": "5.4.1",
        "react": "16.11.0",
        "react-dom": "16.11.0",
        "react-router-dom": "^5.1.2",
        "rimraf": "^3.0.2",
        "semantic-release": "^17.2.1",
        "static-server": "^2.2.1",
        "typescript": "^4.1.3",
        "webpack-dev-server": "^3.11.0"
    },
    "dependencies": {
        "@juggle/resize-observer": "^3.1.3",
        "@tef-novum/webview-bridge": "^2.18.1",
        "@telefonica/libphonenumber": "^2.7.5",
        "classnames": "^2.2.6",
        "focus-trap-react": "^4.0.0",
        "jss": "^10.4.0",
        "jss-plugin-camel-case": "^10.4.0",
        "jss-plugin-default-unit": "^10.4.0",
        "jss-plugin-nested": "^10.4.0",
        "jss-plugin-rule-value-function": "^10.4.0",
        "jss-plugin-vendor-prefixer": "^10.4.0",
        "lodash": "^4.17.20",
        "moment": "^2.29.1",
        "react-autosuggest": "^9.4.3",
        "react-datetime": "^3.0.4",
        "react-jss": "^10.4.0",
        "react-transition-group": "^2.5.3",
        "semver-compare": "^1.0.0"
    },
    "peerDependencies": {
        "react": ">=16.11",
        "react-dom": ">=16.11"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "sideEffects": [
        "*.css"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,css,md,yml,yaml,ts,tsx,d.ts,json}": "prettier --write"
    },
    "release": {
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            "@semantic-release/changelog",
            "@semantic-release/npm",
            "@semantic-release/git",
            "@semantic-release/github"
        ]
    },
    "resolutions": {
        "react": "16.11.0",
        "react-dom": "16.11.0"
    }
}
