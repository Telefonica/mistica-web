// @flow

export type CardOptions = {
  americanExpress?: boolean,
  visa?: boolean,
  masterCard?: boolean,
};
/**
 * Returns true if number looks like an American Express card
 * Do not use for validation
 */
declare export var isAmericanExpress: (str?: string) => boolean;
/**
 * Returns true if number looks like a Visa card
 * Do not use for validation
 */
declare export var isVisa: (str?: string) => boolean;
/**
 * Returns true if number looks like a MasterCard card
 * Do not use for validation
 */
declare export var isMasterCard: (str?: string) => boolean;
declare export var getCvvLength: (creditCardNumber: string) => number;
declare export var getCreditCardNumberLength: (
  creditCardNumber: string
) => number;
declare export var isValidCreditCardNumber: (
  creditCardNumber: string
) => boolean;
